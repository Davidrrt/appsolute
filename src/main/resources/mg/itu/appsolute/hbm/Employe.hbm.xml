<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="mg.itu.appsolute.modele.Employe" table="Employe">
    <meta attribute="class-description">
             
        </meta>
    <id name="id" column="idemp"  type="int">
      <generator class="native">
        <param name="sequence">employe_idemp_seq</param>
      </generator>
    </id>
    <property column="nomemp" name="nomemp" type="string"/>
    <property column="metier" name="metier" type="string"/>
    <property column="login" name="login" type="string"/>
    <property column="pass" name="pass" type="string"/>
    <property column="statut" name="statut" type="int"/>
  </class>
</hibernate-mapping>
